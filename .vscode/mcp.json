{
  "servers": {
    "myCustomPythonAgent": {
      "type": "stdio",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": [
        "${workspaceFolder}\\mcp_server_wrapper.py"
      ],
      "env": {
        "GEMINI_API_KEY": "${env:GEMINI_API_KEY}",
        "MCP_AGENT_API_KEY": "${env:GEMINI_API_KEY}",
        "PYTHONPATH": ""
      },
      "cwd": "${workspaceFolder}"
    },
    "superAlitaBuiltinAgent": {
      "type": "stdio",
      "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
      "args": [
        "${workspaceFolder}\\src\\vscode_integration\\agent_mcp_server.py"
      ],
      "env": {
        "WORKSPACE_FOLDER": "${workspaceFolder}",
        "VSCODE_EXTENSION_MODE": "mcp-provider",
        "PYTHONPATH": "${workspaceFolder}"
      },
      "cwd": "${workspaceFolder}"
    }
  }
}
